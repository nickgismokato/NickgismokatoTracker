	<Project Sdk="Microsoft.NET.Sdk">

		<PropertyGroup>
			<OutputType>WinExe</OutputType>
			<TargetFramework>net8.0-windows</TargetFramework>
			<Nullable>enable</Nullable>
			<ImplicitUsings>enable</ImplicitUsings>
			<UseWPF>true</UseWPF>
			<AssemblyTitle>NickgismokatoTracker</AssemblyTitle>
			<Product>NickgismokatoTracker</Product>
			<Description>A modern media tracking application for movies, series, and anime</Description>
			<Version>1.0.0</Version>
			<FileVersion>1.0.0.0</FileVersion>
			<AssemblyVersion>1.0.0.0</AssemblyVersion>
		</PropertyGroup>

	<ItemGroup>
		<Content Include="bin\Debug\net8.0-windows\NickgismokatoTracker.deps.json" />
		<Content Include="bin\Debug\net8.0-windows\NickgismokatoTracker.runtimeconfig.json" />
	</ItemGroup>

	<ItemGroup>
		<None Include="bin\Debug\net8.0-windows\NickgismokatoTracker.dll" />
		<None Include="bin\Debug\net8.0-windows\NickgismokatoTracker.exe" />
		<None Include="bin\Debug\net8.0-windows\NickgismokatoTracker.pdb" />
	</ItemGroup>
	<ItemGroup>
		<None Update="icon.ico">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

	</Project>
